<xml>

<set name="USE_PRECOMPILED_HEADERS" value="gcc"/>

<linker id="static" exe="ar" unless="windows">
  <ext value=".a"/>
  <outflag value="-cr "/>
  <ranlib name="ranlib"/>
</linker>

<set name="nostrip" value="1" if="debug"/>

<stripper id="strip" exe="strip" unless="nostrip">
  <exe name="strip"/>
</stripper>



</xml>
